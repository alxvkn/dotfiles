#!/bin/sh
test -e ~/.cache/wal/colors.sh && . ~/.cache/wal/colors.sh
exec /usr/bin/fuzzel \
    --background=${background}dd \
    --text-color=${foreground}ff \
    --match-color=${color1}ff \
    --selection-color=${foreground}ff \
    --selection-match-color=${color8}ff \
    --selection-text-color=${background}ff \
    --border-color=${foreground}ff \
    "$@"
